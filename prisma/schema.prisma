generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model oauth {
  id                Int                @id @default(autoincrement())
  id_user           Int?
  connection_type   String?            @db.VarChar(255)
  access_token      String?            @db.VarChar(1000)
  refresh_token     String             @db.VarChar(1000)
  client_secret     String             @db.VarChar(255)
  client_id         String             @db.VarChar(255)
  created_at        DateTime?          @default(now()) @db.Timestamp(6)
  e_connection_type e_connection_type? @relation(fields: [connection_type], references: [name], onDelete: Restrict, map: "oauth_e_connection_type_fk")
  users             users?             @relation(fields: [id_user], references: [id], onDelete: NoAction, map: "oauth_users_fk")
}

model users {
  id         Int       @id @default(autoincrement())
  email      String    @unique @db.VarChar(255)
  password   String
  is_active  Boolean?  @default(true)
  created_at DateTime? @default(now()) @db.Timestamp(6)
  oauth      oauth[]
  track      track[]
}

model e_connection_type {
  name  String  @id(map: "e_connection_type_pk") @db.VarChar
  oauth oauth[]
}

model asset {
  id                              String       @id @db.Char(11)
  name                            String       @db.VarChar
  type                            String       @db.VarChar
  created_at                      DateTime     @default(now()) @db.Timestamp(6)
  beatstars_id                    String?      @db.VarChar
  s3_key                          String       @db.VarChar
  e_asset_type                    e_asset_type @relation(fields: [type], references: [name], map: "asset_e_asset_type_fk")
  track_track_id_beatToasset      track[]      @relation("track_id_beatToasset")
  track_track_id_thumbnailToasset track[]      @relation("track_id_thumbnailToasset")
}

model e_asset_type {
  name  String  @id(map: "e_asset_type_pk") @db.VarChar
  asset asset[]
}

model track {
  id                              String    @id(map: "track_pk") @db.Char(11)
  name                            String    @db.VarChar
  id_user                         Int
  id_beat                         String?   @db.Char(11)
  id_thumbnail                    String?   @db.Char(11)
  yt_url                          String?   @db.VarChar
  created_at                      DateTime  @default(now()) @db.Timestamp(6)
  beatstars_url                   String?   @db.VarChar
  publish_at                      DateTime? @db.Timestamp(6)
  beatstars_id_track              String?   @db.VarChar
  error_message                   String?   @db.VarChar
  asset_track_id_beatToasset      asset?    @relation("track_id_beatToasset", fields: [id_beat], references: [id], onDelete: Cascade, map: "beat_fk")
  asset_track_id_thumbnailToasset asset?    @relation("track_id_thumbnailToasset", fields: [id_thumbnail], references: [id], onDelete: Cascade, map: "thumnbnail_fk")
  users                           users     @relation(fields: [id_user], references: [id], onDelete: Cascade, map: "track_users_fk")
}
