generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model oauth {
  id                Int                @id @default(autoincrement())
  id_user           Int?
  connection_type   String?            @db.VarChar(255)
  access_token      String?            @db.VarChar(1000)
  refresh_token     String             @db.VarChar(1000)
  client_secret     String             @db.VarChar(255)
  client_id         String             @db.VarChar(255)
  created_at        DateTime?          @default(now()) @db.Timestamp(6)
  e_connection_type e_connection_type? @relation(fields: [connection_type], references: [name], onDelete: Restrict, map: "oauth_e_connection_type_fk")
  users             users?             @relation(fields: [id_user], references: [id], onDelete: NoAction, map: "oauth_users_fk")
}

model users {
  id         Int       @id @default(autoincrement())
  email      String    @unique @db.VarChar(255)
  password   String
  is_active  Boolean?  @default(true)
  created_at DateTime? @default(now()) @db.Timestamp(6)
  oauth      oauth[]
}

model e_connection_type {
  name  String  @id(map: "e_connection_type_pk") @db.VarChar
  oauth oauth[]
}
