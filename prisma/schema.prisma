generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model oauth {
  id                  Int                   @id @default(autoincrement())
  id_user             Int?
  connection_type     String?               @db.VarChar(255)
  access_token        String?               @db.VarChar(1000)
  refresh_token       String                @db.VarChar(1000)
  client_secret       String                @db.VarChar(255)
  client_id           String                @db.VarChar(255)
  created_at          DateTime?             @default(now()) @db.Timestamp(6)
  e_connection_type   e_connection_type?    @relation(fields: [connection_type], references: [name], onDelete: Restrict, map: "oauth_e_connection_type_fk")
  user                users?                @relation(fields: [id_user], references: [id], onDelete: NoAction, map: "oauth_users_fk")
  profile_connections profile_connections[]
}

model e_connection_type {
  name                String                @id(map: "e_connection_type_pk") @db.VarChar
  oauth               oauth[]
  profile_connections profile_connections[]
}

model e_asset_type {
  name  String  @id(map: "e_asset_type_pk") @db.VarChar
  asset asset[]
}

model users {
  id         Int        @id @default(autoincrement())
  email      String     @unique @db.VarChar(255)
  password   String
  is_active  Boolean?   @default(true)
  created_at DateTime?  @default(now()) @db.Timestamp(6)
  settings   Json?
  asset      asset[]
  oauth      oauth[]
  profiles   profiles[]
  tracks     track[]
}

model asset {
  id                  String       @id @db.Char(11)
  name                String       @db.VarChar
  type                String       @db.VarChar
  created_at          DateTime     @default(now()) @db.Timestamp(6)
  beatstars_id        String?      @db.VarChar
  s3_key              String       @db.VarChar
  mimetype            String?      @db.VarChar
  id_user             Int?
  id_profile          String?      @db.Char(11)
  e_asset_type        e_asset_type @relation(fields: [type], references: [name], map: "asset_e_asset_type_fk")
  profiles            profiles?    @relation(fields: [id_profile], references: [id], onDelete: Cascade, map: "asset_profiles_fk")
  users               users?       @relation(fields: [id_user], references: [id], onDelete: Cascade, map: "asset_users_fk")
  tracks_as_beat      track[]      @relation("track_beat")
  tracks_as_thumbnail track[]      @relation("track_thumbnail")
}

model track {
  id                 String    @id(map: "track_pk") @db.Char(11)
  name               String    @db.VarChar
  id_user            Int
  id_beat            String?   @db.Char(11)
  id_thumbnail       String?   @db.Char(11)
  yt_url             String?   @db.VarChar
  created_at         DateTime  @default(now()) @db.Timestamp(6)
  beatstars_url      String?   @db.VarChar
  publish_at         DateTime  @default(now()) @db.Timestamp(6)
  beatstars_id_track String?   @db.VarChar
  error_message      String?   @db.VarChar
  id_profile         String?   @db.Char(11)
  beat               asset?    @relation("track_beat", fields: [id_beat], references: [id], onDelete: Cascade, map: "beat_fk")
  thumbnail          asset?    @relation("track_thumbnail", fields: [id_thumbnail], references: [id], onDelete: Cascade, map: "thumnbnail_fk")
  profiles           profiles? @relation(fields: [id_profile], references: [id], onDelete: Cascade, map: "track_profiles_fk")
  user               users     @relation(fields: [id_user], references: [id], onDelete: Cascade, map: "track_users_fk")
}

model profiles {
  id                  String                @id @db.Char(11)
  name                String                @db.VarChar
  id_user             Int
  settings            Json?
  asset               asset[]
  profile_connections profile_connections[]
  users               users                 @relation(fields: [id_user], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "profiles_users_fk")
  track               track[]
}

model profile_connections {
  id                String            @id(map: "profile_connection_pkey") @default(dbgenerated("gen_random_uuid()")) @db.Char(11)
  id_profile        String            @db.Char(11)
  id_oauth          Int
  platform          String            @db.VarChar(50)
  meta              Json?
  is_active         Boolean?          @default(true)
  created_at        DateTime          @default(now()) @db.Timestamp(6)
  e_connection_type e_connection_type @relation(fields: [platform], references: [name], onDelete: Cascade, map: "profile_connection_e_connection_type_fk")
  oauth             oauth             @relation(fields: [id_oauth], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "profile_connection_id_oauth_fkey")
  profiles          profiles          @relation(fields: [id_profile], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "profile_connection_id_profile_fkey")

  @@unique([id_profile, platform], map: "profile_connection_profile_platform")
  @@index([id_oauth], map: "idx_profile_connection_oauth")
  @@index([id_profile], map: "idx_profile_connection_profile")
}
